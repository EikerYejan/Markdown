{
  "parser": "@typescript-eslint/parser",
  "extends": ["react-app", "plugin:@typescript-eslint/recommended", "airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "quotes": 0,
    "linebreak-style": 0,
    "import/no-unresolved": 0,
    "object-curly-newline": 0,
    "implicit-arrow-linebreak": 0,
    "no-unused-expressions": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }],
    "import/extensions": [
      "error",
      {
        "ts": "always",
        "tsx": "always",
        "js": "never"
      }
    ],
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
